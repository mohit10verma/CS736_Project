enum enc_dec_method {
    USE_SAMPLE_CODE=0x01,
    USE_iAES       =0x02,
    USE_iAES_BLK   =0x04,
    USE_iAES_CBC   =0x08,
    USE_iAES_CTR   =0x10,
    USE_ARRAY      =0x20,
    // if you add something to this enum then you need to add it (except for the USE_ARRAY entry) to the
    // check_sizes_and_methods() routine 'method[]' array
    //
};

typedef struct aes_param{
	char* bmark_file_name; int bmark_key_size; int bmark_file_size; int bmark_encrypt; int bmark_loops; int outer_loops
}aes_params;
